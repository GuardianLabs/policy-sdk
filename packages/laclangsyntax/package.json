{
  "name": "laclangsyntax",
  "displayName": "LacLangSyntax",
  "description": "Syntax highlighting for LacLang",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "laclang",
      "aliases": ["LacLang", "laclang"],
      "extensions": [".lac"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "laclang",
      "scopeName": "source.laclang",
      "path": "./syntaxes/laclang.tmLanguage.json"
    }]
  }
}
