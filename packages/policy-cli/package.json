{
  "name": "@guardian-network/policy-cli",
  "author": "",
  "description": "CLI for an SDK",
  "version": "0.1.0",
  "main": "index.ts",
  "engines": {
    "node": ">=18.20.4"
  },
  "bin": {
    "compile": "./dist/policy-cli/compiler/compile.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig-prod.json && pnpm format:all:fix && pnpm copy-dts",
    "build:dev": "tsc -p tsconfig.json && pnpm format:all:fix && pnpm copy-dts",
    "clean": "pnpm remove:dist",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" \"compiler/**/*.d.ts\" ./dist",
    "format:all:fix": "pnpm format:ts:fix",
    "format:ts:fix": "prettier -w -c \"src/**/*.ts\" \"test/**/*.ts\" \"compiler/**/*.ts\"",
    "remove:dist": "npx rimraf dist",
    "test": "mocha",
    "compile": "tsx ./compiler/compile.ts"
  },
  "keywords": [
    "cli",
    "policy",
    "laclang",
    "sdk",
    "blockchain"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "chai": "^4.5.0",
    "commander": "^12.1.0",
    "copyfiles": "^2.4.1",
    "ethers": "^6.13.3",
    "mocha": "^10.8.2",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.8.0",
    "dotenv": "^16.4.5",
    "tsx": "^4.19.2"
  }
}
